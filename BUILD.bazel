load("@io_bazel_rules_docker//python3:image.bzl", "py3_image")
load("@pip_deps//:requirements.bzl", "requirement")

exports_files(["requirements.txt"])

py3_image(
    name = "dbt",
    srcs = ["demo.py"],
    main = "demo.py",
    deps = [requirement("dbt")],
)

py3_image(
    name = "numpy",
    srcs = ["numpy-test.py"],
    main = "numpy-test.py",
    deps = [requirement("numpy")],
)


